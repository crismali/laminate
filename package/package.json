{
  "name": "laminate",
  "version": "0.0.6",
  "description": "Route stack components for React Router v4",
  "keywords": [
    "component",
    "react",
    "router",
    "stack",
    "native",
    "react-native"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "Stack",
    "AnimatedRoute",
    "SwipeCatcher",
    "Animation.js"
  ],
  "repository": "https://github.com/devmynd/laminate",
  "author": "Michael Crismali",
  "license": "MIT",
  "scripts": {
    "clean": "git clean -fdX",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "yarn lint && yarn test",
    "verify-and-publish": "yarn validate && yarn publish"
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "jest": {
    "clearMocks": true,
    "resetModules": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "./jest/setup.js"
    ],
    "modulePaths": [
      "<rootDir>/jest/"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(react-native|react-router-native)/"
    ]
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.3.1",
    "lodash.some": "^4.6.0",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-router-native": ">=4.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-preset-react-native": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.16.0",
    "eslint-config-devmynd": "^0.1.2",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.2.2",
    "jest-enzyme": "^4.2.0",
    "lodash": "^4.17.5",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-native": "^4.2.0",
    "react-test-renderer": "^16.3.0"
  }
}
